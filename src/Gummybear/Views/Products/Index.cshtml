@model IEnumerable<Gummybear.Models.Product>

@{
    ViewData["Title"] = "Travel Blog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h2>Products</h2>
    @foreach(var product in Model)
    {
        <p>@Html.DisplayFor(modelItem => product.Name)</p>
    }

    <p>@Html.ActionLink("Add new item", "Create")</p>
</div>
